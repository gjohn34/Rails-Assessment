<%= simple_form_for @profile do |f| %>
  <div class='row'>
    <div class='col s12 m6'
      <h6>Profile Pic</h6>
      <%= f.file_field :pic%>
      <%= f.input :name %>
      <%= f.input :bio %>
      <%= f.button :submit %>
    </div>
    <div class='col s12 m6'
    <span>interest stuff</span><br>
      <%= form_for @profile do |f| %>
        <%= f.collection_check_boxes :interests_id, Interest.all, :id, :interest, as: :boolean %>
      <% end %>
    </div>
    <br>
<% end %>
